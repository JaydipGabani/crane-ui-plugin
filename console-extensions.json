[
  {
    "type": "console.page/route",
    "properties": {
      "exact": true,
      "path": "/import-application/ns/:namespace",
      "component": { "$codeRef": "ImportWizardPage" }
    }
  },
  {
    "type": "dev-console.add/action-group",
    "properties": {
      "id": "smart-import",
      "name": "Smart Import",
      "insertAfter": "container-images"
    }
  },
  {
    "type": "dev-console.add/action",
    "properties": {
      "id": "import-application",
      "groupId": "smart-import",
      "label": "Import application",
      "description": "Import an application and associated resources from this cluster or a remote cluster",
      "href": "/import-application/ns/:namespace",
      "icon": { "$codeRef": "icons.importIconElement" }
    }
  }
]
